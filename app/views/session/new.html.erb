<!-- Login Page -->

<% if logged_in? %>
<div class="new_div">
	
</div>


<% else %>

<ul>
    <%= form_tag("/session", method: "post") do %>
      <li><%= label_tag(:email) %></li>
      <%= text_field_tag(:email) %>
      <li><%= label_tag(:password) %></li>
      <%= password_field_tag(:password) %>
      <li><%= submit_tag("Login") %></li>
      <% end %>

</ul>

<% end %>
